{
  "name": "stratford_ai",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:netlify": "next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "export": "next build && next export",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run type-check && npm run lint && npm run test && npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out dist",
    "precommit": "npm run format && npm run lint:fix && npm run type-check && npm run test"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/ioredis": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "critters": "^0.0.25",
    "csv-parse": "5.4.0",
    "ioredis": "^5.8.0",
    "lucide-react": "0.295.0",
    "mongoose": "^8.18.3",
    "next": "14.0.4",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "prisma": "^6.16.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.47.0",
    "recharts": "2.8.0",
    "redis": "^5.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.29.1",
    "tailwind-merge": "2.2.0",
    "undici": "5.28.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@vitest/ui": "1.0.4",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "23.0.1",
    "msw": "1.3.2",
    "netlify-plugin-pnpm": "^1.0.4",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "tailwindcss": "3.3.6",
    "tailwindcss-animate": "1.0.7",
    "ts-jest": "^29.4.4",
    "typescript": "5.2.2",
    "vitest": "1.0.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}